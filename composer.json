{
    "name": "tzepart/test-separator",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/TzepART/test-separator.git",
    "description": "Project for separating of tests",
    "keywords": [
        "tests",
        "codeception",
        "tests groups",
        "allure"
    ],
    "authors": [
        {
            "name": "Tsepkov Artem",
            "email": "tzepart@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TestSeparator\\": "src/",
            "Tests\\" : "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "drupol/phpartition": "^0.1.2",
        "symfony/console": "^4.4",
        "ext-json": "*",
        "ext-simplexml": "*",
        "codeception/codeception": "^4.0",
        "symfony/finder": "^4.4"
    },
    "bin": [
        "separate-tests"
    ],
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7.5"
    }
}
